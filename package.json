{
    "name": "discord-bot-admin",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "pm2": "pm2 start npm --name Discord_React -- start",
        "serve": "PORT=80 serve -s build"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.3",
        "axios": "^0.21.1",
        "bootstrap": "^4.5.3",
        "discord-oauth2": "^2.6.0",
        "react": "^17.0.1",
        "react-bootstrap": "^1.4.3",
        "react-dom": "^17.0.1",
        "react-icons": "^4.1.0",
        "react-json-view": "^1.20.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.1",
        "sweetalert2": "^10.13.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.3",
        "@testing-library/user-event": "^12.6.0",
        "eslint": "^7.17.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest",
            "eslint:recommended"
        ],
        "rules": {
            "semi": "warn",
            "quotes": [
                "warn",
                "single"
            ],
            "no-trailing-spaces": "warn",
            "camelcase": "warn",
            "eol-last": "warn",
            "no-var": "warn",
            "prefer-const": "warn",
            "consistent-return": "error",
            "indent": [
                "error",
                4
            ],
            "comma-dangle": ["error", {
                "arrays": "always",
                "objects": "always",
                "imports": "never",
                "exports": "never",
                "functions": "never"
            }]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
