{
    "name": "discord-bot-admin-backend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "dev": "nodemon ./bin/www",
        "pm2": "pm2 start --name Site_BackEnd npm -- start"
    },
    "dependencies": {
        "btoa": "^1.2.1",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "eslint": "^7.18.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.22.1",
        "express": "~4.16.1",
        "express-fileupload": "^1.2.1",
        "http-errors": "~1.6.3",
        "jade": "^1.9.2",
        "morgan": "~1.9.1",
        "mysql2": "^2.2.5",
        "node-fetch": "^2.6.1"
    },
    "devDependencies": {
        "nodemon": "^2.0.7"
    },
    "eslintConfig": {
        "extends": "airbnb-base",
        "rules": {
            "semi": "warn",
            "quotes": 0,
            "no-trailing-spaces": "warn",
            "camelcase": "warn",
            "eol-last": "warn",
            "no-var": "warn",
            "prefer-const": "warn",
            "consistent-return": "off",
            "no-unused-vars": "off",
            "no-use-before-define": "off",
            "indent": 0,
            "operator-linebreak": 0
        },
        "env": {
            "es6": true,
            "browser": true
        }
    }
}
