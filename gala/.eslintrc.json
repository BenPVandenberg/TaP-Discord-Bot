{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "semi": "warn",
        "quotes": 0,
        "no-trailing-spaces": "warn",
        "camelcase": "error",
        "eol-last": "warn",
        "no-var": "warn",
        "prefer-const": "warn",
        "consistent-return": "off",
        "no-unused-vars": "off",
        "no-use-before-define": "off",
        "indent": 0,
        "operator-linebreak": 0
    }
}
